var Build = xdc.useModule('xdc.bld.BuildEnvironment');

var GCC = xdc.useModule('gnu.targets.Linux86');
GCC.rootDir = '/usr';
GCC.ccOpts.prefix = "-O0 -ggdb -DDUMP_EVENT";

var GCCARM = xdc.useModule('gnu.targets.arm.M4F');
GCCARM.rootDir ='/usr';
GCCARM.ccOpts.prefix = "-O0 -ggdb";

/* GCCARM doesn't link: looks like this is due to imcomplete support for TivaC
 * target -- no (or incomplete) linker script. */

Build.targets = [GCC];
